{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "no-extra-semi": 0,

    "quotes": ["error", "double"],
    "no-prototype-builtins": "off",
    "consistent-return": "off",
    "no-plusplus": "off"
  }
}
